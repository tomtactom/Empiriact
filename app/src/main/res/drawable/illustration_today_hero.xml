    android:width="200dp"
    android:height="120dp"
    android:viewportWidth="200"
    android:viewportHeight="120">

    <!-- Surreal sun rising, using new secondary color -->
    <path
        android:fillColor="#FF2A6F97"
        android:pathData="M 20,120 A 80,80 0 0,1 180,120 Z"/>

    <!-- Abstract timeline, using new primary color -->
