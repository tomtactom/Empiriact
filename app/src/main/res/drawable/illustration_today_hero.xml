        android:fillColor="#FFE9C46A"
        android:pathData="M 98,43 a 5,5 0 1,1 0.001,0 Z"/>
    <path
        android:fillColor="#FFE9C46A"
        android:pathData="M 150,58 a 4,4 0 1,1 0.001,0 Z"/>

